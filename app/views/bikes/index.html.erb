<h1>Bikes#index</h1>
<p>Find me in app/views/bikes/index.html.erb</p>

<ul>
  <% @bikes.each do |bike| %>
    <li><%= link_to bike.name, bike_path(bike) %></li>
  <% end %>

  <div style="width: 100%; height: 600px;"
  data-controller="map"
  data-map-markers-value="<%= @markers.to_json %>"
  data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>"></div>
</ul>
