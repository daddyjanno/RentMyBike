<h1>Bikes#new</h1>
<p>Find me in app/views/bikes/new.html.erb</p>

<%= simple_form_for @bike do |f| %>
  <%= f.input :name %>
  <%= f.input :category %>
  <%= f.input :model %>
  <%= f.input :location %>
  <%= f.input :description %>
  <%= f.input :price_per_day %>
  <%= f.input :photo, as: :file %>
  <%= f.submit %>
<% end %>
